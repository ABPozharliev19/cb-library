{
  "name": "server",
  "description": "A blank template to get started with Payload",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=cms/payload.config.ts node -r ts-node/register --watch-path ./server.ts server.ts",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=cms/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "npm copyfiles && npm build:payload && npm build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"cms/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=cms/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=cms/payload.config.ts payload generate:graphQLSchema",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=cms/payload.config.ts payload",
    "test": "jest"
  },
  "dependencies": {
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@payloadcms/bundler-webpack": "^1.0.0",
    "@payloadcms/db-postgres": "^0.x",
    "@payloadcms/plugin-cloud": "^2.0.0",
    "@payloadcms/richtext-slate": "^1.0.0",
    "@radix-ui/themes": "^2.0.3",
    "@remix-run/css-bundle": "^2.4.1",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@remix-run/serve": "^2.4.1",
    "@types/jest": "^29.5.11",
    "cron": "^3.1.6",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "isbot": "^3.7.1-deprecated",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-mock-strategy": "^2.0.0",
    "passport-oauth2": "^1.7.0",
    "payload": "^2.0.0",
    "pino-pretty": "^10.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "url": "^0.11.3",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@remix-run/dev": "^2.4.1",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.15",
    "babel-jest": "^29.7.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.54.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-react": "^7.33.2",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.8.4",
    "vite": "^5.0.10"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}
