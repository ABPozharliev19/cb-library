{
  "name": "server",
  "description": "A blank template to get started with Payload",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "npm copyfiles && npm build:payload && npm build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"cms/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "generate:templates": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts ts-node src/actions/convertEmailTemplates.ts",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "test": "jest"
  },
  "dependencies": {
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@meilisearch/instant-meilisearch": "^0.14.0",
    "@payloadcms/bundler-webpack": "^1.0.0",
    "@payloadcms/db-postgres": "^0.x",
    "@payloadcms/plugin-cloud": "^2.0.0",
    "@payloadcms/richtext-slate": "^1.0.0",
    "@types/jest": "^29.5.11",
    "cron": "^3.1.6",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint-plugin-import": "^2.29.1",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "handlebars": "^4.7.8",
    "isbot": "^3.7.1-deprecated",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "meilisearch": "^0.36.0",
    "mjml": "^4.15.2",
    "passport": "^0.6.0",
    "passport-mock-strategy": "^2.0.0",
    "passport-oauth2": "^1.7.0",
    "payload": "^2.5.0",
    "pino-pretty": "^10.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "url": "^0.11.3",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mjml": "^4.7.4",
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.15",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "babel-jest": "^29.7.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.54.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "i": "^0.3.7",
    "nodemon": "^2.0.6",
    "npm": "^10.2.5",
    "postcss": "^8.4.33",
    "postcss-preset-mantine": "^1.12.3",
    "postcss-simple-vars": "^7.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.8.4",
